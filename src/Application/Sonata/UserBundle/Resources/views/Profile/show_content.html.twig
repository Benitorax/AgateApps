{% trans_default_domain 'FOSUserBundle' %}

<ol class="breadcrumb">
    <li><a href="{{ url('pierstoval_admin_admin_index') }}">{{ 'Accueil' | trans }}</a></li>
    <li>{{ 'Voir le profil' | trans }}</li>
</ol>

<h1>{{ 'Voir le profil' | trans }}</h1>

<div class="fos_user_user_show">
    <div class="row">
        <div class="col-sm-3 text-right">{{ 'profile.show.username'|trans }}</div>
