<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="corahn_rin_characters.sheets_manager.class">CorahnRin\CharactersBundle\Sheets\SheetsService</parameter>
    </parameters>

    <services>
        <service id="corahn_rin_characters.sheets_manager" class="%corahn_rin_characters.sheets_manager.class%">
            <argument>%corahn_rin_characters.sheets_folder%</argument>
            <argument type="service" id="translator" />
            <argument type="service" id="kernel" />
        </service>
    </services>

</container>
