{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "egeloen/ckeditor-bundle": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "^2.1",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "nelmio/cors-bundle": "^1.5",
        "orbitale/cms-bundle": "^3.0",
        "orbitale/doctrine-tools": "^0.5.6",
        "orbitale/entity-merger": "^0.6.0",
        "orbitale/imagemagick-php": "^1.6",
        "pierstoval/character_manager": "^0.5.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^4.0|^5.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/console": "^3.3",
        "symfony/form": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/security": "^3.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/yaml": "^3.3",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "symfony/flex": "^1.0",
        "symfony/dotenv": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BWX3FPWM0F114CXPMK8WTWR6",
            "allow-contrib": true
        }
    }
}
