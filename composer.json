{
    "name": "esteren/esteren",
    "license": "proprietary",
    "description": "Global Esteren project",
    "type": "project",
    "require": {
        "php": "^7.1",
        "ext-apcu": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "doctrine/common": "^2.8",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "easycorp/easyadmin-bundle": "^1.17",
        "google/recaptcha": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "^2.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nelmio/cors-bundle": "^1.5",
        "oneup/flysystem-bundle": "^3.0",
        "orbitale/doctrine-tools": "^0.6.1",
        "orbitale/imagemagick-php": "^2.0",
        "pierstoval/character-manager": "^1.1.3",
        "sensio/framework-extra-bundle": "^5.0",
        "sensiolabs/security-checker": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/force-lowest": "^4.1",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/process": "^4.0",
        "symfony/security": "^4.0",
        "symfony/serializer": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "ext-zip": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "pierstoval/web-test-case": "^0.2.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/stopwatch": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "autoloader-suffix": "EsterenPortal",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/Admin",
            "Agate\\": "src/Agate",
            "CorahnRin\\": "src/CorahnRin",
            "Dragons\\": "src/Dragons",
            "Esteren\\": "src/Esteren",
            "EsterenMaps\\": "src/EsterenMaps",
            "Util\\": "src/Util"
        },
        "classmap": [
            "src/Kernel.php"
        ],
        "exclude-from-classmap": [
            "/src/*/vendor/",
            "/src/*/*/vendor/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Admin\\": "tests/Admin",
            "Tests\\Agate\\": "tests/Agate",
            "Tests\\CorahnRin\\": "tests/CorahnRin",
            "Tests\\Dragons\\": "tests/Dragons",
            "Tests\\Esteren\\": "tests/Esteren",
            "Tests\\EsterenMaps\\": "tests/EsterenMaps"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-warmup": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "npm run deploy",
            "php bin/console doctrine:schema:update --dump-sql --force",
            "php bin/console doctrine:schema:validate || echo 'Doctrine schema not valid, please make sure it is correct.'",
            "php bin/console esterenmaps:map:generate-tiles 1"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BWX3FPWM0F114CXPMK8WTWR6",
            "allow-contrib": true
        }
    },
    "authors": [
