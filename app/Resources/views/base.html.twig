<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{% endblock %}</title>
        <meta name="generator" value="Corahn Rin {% 
            render(controller("CorahnRinPagesBundle:Versions:get", { "filter" : "code" }))
        %} - Automatic character creation by Pierstoval" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
{% block more_headers %}{% endblock %}
        {% block stylesheets %}{% endblock %}

{% stylesheets
'../vendor/twitter/bootstrap/dist/css/bootstrap.css'
'../vendor/twitter/bootstrap/dist/css/bootstrap-theme.css'
'@CorahnRinPagesBundle/Resources/public/css/jquery-ui.css'
'@CorahnRinPagesBundle/Resources/public/css/icons-large.css'
'@CorahnRinPagesBundle/Resources/public/css/main.css'
%}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
{% endstylesheets %}
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
{% javascripts
'../vendor/twitter/bootstrap/js/tests/vendor/jquery.js'
'../vendor/twitter/bootstrap/dist/js/bootstrap.js'
%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
    </body>
</html>
